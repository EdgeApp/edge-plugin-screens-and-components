{
  "presets": [
    ["@babel/preset-env", { "loose": true }],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "@babel/plugin-transform-runtime",
      { "corejs": false, "helpers": false, "regenerator": true }
    ]
  ]
}
